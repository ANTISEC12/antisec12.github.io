{
  "name": "kikobeats.com",
  "description": "ma website yo",
  "version": "1.1.19",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "contributors": [
    {
      "name": "Anand Dyavanapalli",
      "email": "adyavanapalli@gmail.com"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "acho": "latest",
    "browser-sync": "latest",
    "condense-whitespace": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "gulp": "latest",
    "husky": "latest",
    "imageoptim-cli": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "svgo": "latest",
    "tachyons-sass": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "NODE_ENV=production JEKYLL_ENV=production bundle exec jekyll build",
    "dev": "gulp",
    "lint": "standard-markdown && standard",
    "postrelease": "npm run release:tags && npm run release:github",
    "precommit": "lint-staged",
    "prerelease": "npm run update:check && git-authors-cli",
    "pretest": "npm run lint",
    "release": "git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "exit 0",
    "update": "ncu -a",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "finepack",
        "git add"
      ],
      "*.js": [
        "prettier-standard",
        "git add"
      ],
      "*.md": [
        "standard-markdown",
        "git add"
      ],
      "*.{png,jpeg,jpg,gif}": [
        "imageoptim --imagealpha",
        "git add"
      ],
      "*.svg": [
        "svgo --pretty",
        "git add"
      ]
    }
  },
  "standard": {
    "ignore": [
      "js/prism.js"
    ]
  },
  "standard-version": {
    "scripts": {
      "prechangelog": "git-authors-cli"
    }
  },
  "urlint": {
    "url": "https://kikobeats.com/sitemap.xml",
    "whitelist": [
      "https://www.linkedin.com/in/kikobeats"
    ]
  }
}
